{
  "framework": "vite",
  "installCommand": "npm install --platform=linux --arch=x64",
  "buildCommand": "rm -rf node_modules && npm install && npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "github": {
    "silent": true
  }
}
