{
  "framework": "vite",
  "installCommand": "npm install && npm install @rollup/rollup-linux-x64-gnu || npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "github": {
    "silent": true
  }
}
